<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.questionnaire">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_faq_"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_faq_"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".EditQuestion" />
        <activity android:name=".ViewQuestions" />
        <activity android:name=".AddNewQuestion" />
        <activity android:name=".QuestionsActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>